{
    "name": "daniel-km/omeka-s-module-single-sign-on",
    "type": "omeka-s-module",
    "description": "Module for Omeka S: Authenticate automatically on Omeka S via single sign-on (SSO) through SAML or Shibboleth and manage multiple identity providers automatically.",
    "keywords": [
        "Omeka S",
        "Omeka S module",
        "login",
        "authentication",
        "SSO",
        "single sign-on",
        "SAML",
        "Shibboleth"
    ],
    "license": "CECILL-2.1",
    "homepage": "https://gitlab.com/Daniel-KM/Omeka-S-module-SingleSignOn",
    "authors": [
        {
            "name": "Daniel Berthereau",
            "email": "Daniel.packagist@Berthereau.net"
        }
    ],
    "support": {
        "issues": "https://gitlab.com/Daniel-KM/Omeka-S-module-SingleSignOn/-/issues",
        "forum": "https://forum.omeka.org/"
    },
    "_require (not set until Omeka S merges PR #2412)": {
        "omeka/omeka-s": "^4.1",
        "daniel-km/omeka-s-module-common": "*"
    },
    "require": {
        "cweagans/composer-patches": "^1.7",
        "onelogin/php-saml": "4.3.1"
    },
    "autoload": {
        "psr-4": {
            "SingleSignOn\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SingleSignOnTest\\": "tests/SingleSignOnTest/"
        }
    },
    "extra": {
        "installer-name": "SingleSignOn",
        "configurable": true,
        "patches": {
            "onelogin/php-saml": {
                "Fix decryptNameId algorithm mismatch (issue #627)": "data/patches/fix-decrypt-nameid.patch"
            }
        }
    },
    "config": {
        "allow-plugins": {
            "cweagans/composer-patches": true
        }
    }
}
